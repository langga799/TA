get.ConstraintLayout>        </com.google.android.material.card.MaterialCardView>        <com.google.android.material.card.MaterialCardView            android:id="@+id/materialCardView2"            android:layout_width="370dp"            android:layout_height="200dp"            android:layout_marginTop="16dp"            app:cardBackgroundColor="@color/Dark"            app:cardCornerRadius="24dp"            app:layout_constraintEnd_toEndOf="parent"            app:layout_constraintStart_toStartOf="parent"            app:layout_constraintTop_toBottomOf="@+id/materialCardView">            <androidx.constraintlayout.widget.ConstraintLayout                android:layout_width="match_parent"                android:layout_height="match_parent">                <TextView                    android:id="@+id/textView6"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginStart="16dp"                    android:layout_marginTop="16dp"                    android:text="@string/suhu"                    android:textColor="@color/white"                    android:textSize="24sp"                    android:textStyle="bold"                    app:layout_constraintEnd_toStartOf="@+id/progressBarSuhu"                    app:layout_constraintHorizontal_bias="0.0"                    app:layout_constraintStart_toStartOf="parent"                    app:layout_constraintTop_toTopOf="parent" />                <ImageView                    android:layout_width="58dp"                    android:layout_height="92dp"                    android:layout_marginStart="48dp"                    android:src="@drawable/ic_suhulogo"                    app:layout_constraintBottom_toBottomOf="parent"                    app:layout_constraintEnd_toStartOf="@+id/progressBarSuhu"                    app:layout_constraintHorizontal_bias="0.0"                    app:layout_const